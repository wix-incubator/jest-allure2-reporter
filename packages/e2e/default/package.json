{
  "name": "@wix/jest-allure2-reporter-e2e-default",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "jest || true",
    "build": "allure generate --clean",
    "start": "allure serve",
    "test:dev": "npm run build --workspace=jest-allure2-reporter && npm test ; npm run build"
  },
  "devDependencies": {
    "@wix/eslint-config-jest-allure2-reporter": "^1.0.0",
    "@wix/jest-config-jest-allure2-reporter": "^1.0.0",
    "@wix/prettier-config-jest-allure2-reporter": "^1.0.0",
    "@wix/typescript-config-jest-allure2-reporter": "^1.0.0"
  },
  "dependencies": {
    "jest-allure2-reporter": "^1.0.0"
  },
  "prettier": "@wix/prettier-config-jest-allure2-reporter",
  "eslintConfig": {
    "extends": "@wix/jest-allure2-reporter"
  }
}
